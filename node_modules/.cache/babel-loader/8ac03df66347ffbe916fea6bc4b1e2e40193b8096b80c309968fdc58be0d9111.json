{"ast":null,"code":"export default {\n  data() {\n    return {\n      useremail: this.$route.query.useremail,\n      blogItem: null\n    };\n  },\n  mounted() {\n    this.fetchBlogDetails();\n  },\n  methods: {\n    formatTime(timestamp) {\n      const date = new Date(timestamp);\n      const formattedDateTime = date.toLocaleString(\"en-US\", {\n        hour: \"numeric\",\n        minute: \"numeric\"\n      });\n      return formattedDateTime;\n    },\n    formatDate(timestamp) {\n      const date = new Date(timestamp);\n      const formattedDateTime = date.toLocaleString(\"en-US\", {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      });\n      return formattedDateTime;\n    },\n    fetchBlogDetails() {\n      const itemString = this.$route.query.item; // Get the item string from the query parameters\n      if (itemString) {\n        try {\n          const item = JSON.parse(itemString); // Parse the item string into an object\n          // Extract the fields from the parsed item\n          const {\n            name,\n            email,\n            description,\n            createdAt,\n            content,\n            phoneNumber,\n            topImage\n          } = item;\n          // Construct the blogItem object with the extracted fields\n          this.blogItem = {\n            name,\n            user: {\n              email,\n              phoneNumber\n            },\n            description,\n            createdAt,\n            content,\n            topImage\n          };\n        } catch (error) {\n          console.error(\"Error parsing blog item:\", error);\n        }\n      } else {\n        console.error(\"No blog item data found in query parameters.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","useremail","$route","query","blogItem","mounted","fetchBlogDetails","methods","formatTime","timestamp","date","Date","formattedDateTime","toLocaleString","hour","minute","formatDate","weekday","year","month","day","itemString","item","JSON","parse","name","email","description","createdAt","content","phoneNumber","topImage","user","error","console"],"sources":["public/BlogDetailedView.vue"],"sourcesContent":["<template>\n  <div>\n    <v-layout wrap justify-center>\n      <v-flex xs10 pt-3>\n        <v-layout wrap justify-center>\n          <v-flex xs12 lg12>\n            <v-card>\n              <v-layout wrap justify-center pt-3>\n                <v-flex xs12>\n                  <v-layout wrap justify-start>\n                    <v-flex xs12 v-if=\"blogItem && blogItem.topImage\">\n                      <v-layout wrap justify-center>\n                        <v-flex xs12>\n                          <v-img\n                            :src=\"blogItem.topImage\"\n                            height=\"100%\"\n                            width=\"100%\"\n                            contain\n                          ></v-img>\n                        </v-flex>\n                      </v-layout>\n                    </v-flex>\n\n                    <v-flex xs12 v-else>\n                      <v-img\n                        height=\"150px\"\n                        width=\"100%\"\n                        src=\"../src/assets/Images/noimg.png\"\n\n                       \n                      ></v-img>\n                    </v-flex>\n                  </v-layout>\n                </v-flex>\n                <v-flex xs12 sm8 lg12>\n                  <v-layout wrap justify-center fill-height>\n                    <v-flex v-if=\"blogItem && blogItem.name\" xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Blog Name:</strong>\n                      <span style=\"font-size: 14px\">\n                        {{ blogItem.name }}\n                      </span>\n                    </v-flex>\n\n                    <v-flex xs11 lg11 pt-3 align-self-center text-left >\n                      <strong>Added by :</strong>\n                      <span\n                        v-if=\"useremail\"\n                        style=\"font-size: 14px\"\n                      >\n                        {{ useremail}}\n                      </span>\n                    </v-flex>\n\n                    <v-flex  v-if=\"blogItem && blogItem.description\" xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Description :</strong>\n                      <span style=\"font-size: 14px\">\n                        {{ blogItem.description }}\n                      </span>\n                    </v-flex>\n\n                    <v-flex v-if=\"blogItem && blogItem.content\" xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Content :</strong>\n                      <span v-html=\"blogItem.content\" style=\"font-size: 14px\">\n                      </span>\n                    </v-flex>\n\n                    <v-flex xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Phone number :</strong>\n                      <span\n                        v-if=\"\n                          blogItem && blogItem.user && blogItem.user.phoneNumber\n                        \"\n                        style=\"font-size: 14px\"\n                      >\n                        {{ blogItem.user.phoneNumber }}\n                      </span>\n                    </v-flex>\n\n                    <v-flex xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Time :</strong>\n                      <span v-if=\"blogItem && blogItem.createdAt\">\n                        {{ formatTime(blogItem.createdAt) }}\n                      </span>\n                    </v-flex>\n\n                    <v-flex xs11 lg11 pt-3 align-self-center text-left>\n                      <strong>Date :</strong>\n                      <span v-if=\"blogItem && blogItem.createdAt\">\n                        {{ formatDate(blogItem.createdAt) }}\n                      </span>\n                    </v-flex>\n                  </v-layout>\n                </v-flex>\n              </v-layout>\n            </v-card>\n          </v-flex>\n        \n        </v-layout>\n      </v-flex>\n    </v-layout>\n  </div>\n</template>\n  \n<script>\nexport default {\n  data() {\n    return {\n\n        useremail:this.$route.query.useremail,\n      blogItem: null,\n    };\n  },\n  mounted() {\n    this.fetchBlogDetails(); \n  },\n  methods: {\n    formatTime(timestamp) {\n      const date = new Date(timestamp);\n      const formattedDateTime = date.toLocaleString(\"en-US\", {\n        hour: \"numeric\",\n        minute: \"numeric\",\n      });\n      return formattedDateTime;\n    },\n    formatDate(timestamp) {\n      const date = new Date(timestamp);\n      const formattedDateTime = date.toLocaleString(\"en-US\", {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n      });\n      return formattedDateTime;\n    },\n    fetchBlogDetails() {\n      const itemString = this.$route.query.item; // Get the item string from the query parameters\n      if (itemString) {\n        try {\n          const item = JSON.parse(itemString); // Parse the item string into an object\n          // Extract the fields from the parsed item\n          const {\n            name,\n            email,\n            description,\n            createdAt,\n            content,\n            phoneNumber,\n            topImage,\n          } = item;\n          // Construct the blogItem object with the extracted fields\n          this.blogItem = {\n            name,\n            user: { email, phoneNumber },\n            description,\n            createdAt,\n            content,\n            topImage,\n          };\n        } catch (error) {\n          console.error(\"Error parsing blog item:\", error);\n        }\n      } else {\n        console.error(\"No blog item data found in query parameters.\");\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAwGA;EACAA,KAAA;IACA;MAEAC,SAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,SAAA;MACAG,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACAC,WAAAC,SAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,SAAA;MACA,MAAAG,iBAAA,GAAAF,IAAA,CAAAG,cAAA;QACAC,IAAA;QACAC,MAAA;MACA;MACA,OAAAH,iBAAA;IACA;IACAI,WAAAP,SAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,SAAA;MACA,MAAAG,iBAAA,GAAAF,IAAA,CAAAG,cAAA;QACAI,OAAA;QACAC,IAAA;QACAC,KAAA;QACAC,GAAA;MACA;MACA,OAAAR,iBAAA;IACA;IACAN,iBAAA;MACA,MAAAe,UAAA,QAAAnB,MAAA,CAAAC,KAAA,CAAAmB,IAAA;MACA,IAAAD,UAAA;QACA;UACA,MAAAC,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAH,UAAA;UACA;UACA;YACAI,IAAA;YACAC,KAAA;YACAC,WAAA;YACAC,SAAA;YACAC,OAAA;YACAC,WAAA;YACAC;UACA,IAAAT,IAAA;UACA;UACA,KAAAlB,QAAA;YACAqB,IAAA;YACAO,IAAA;cAAAN,KAAA;cAAAI;YAAA;YACAH,WAAA;YACAC,SAAA;YACAC,OAAA;YACAE;UACA;QACA,SAAAE,KAAA;UACAC,OAAA,CAAAD,KAAA,6BAAAA,KAAA;QACA;MACA;QACAC,OAAA,CAAAD,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}